    var faces = ["Spades", "Hearts", "Diamonds", "Clubs"];
    var cardNumbers = ["2", "3", "4", "5", "6", "7", "8", "9", "10", "Jack", "Queen", "King", "Ace"];

    function makeDeck()
    {
        var deck = [];
        for (var i = 0 ; i < cardNumbers.length; i++)
        {
            for(var y = 0; y < faces.length; y++)
            {
            var card = {Value: cardNumbers[y], Face: faces[i]};
			deck.push(card);
            }
        }
        return deck;
    }
